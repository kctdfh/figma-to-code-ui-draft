{
  "name": "figma-to-code-ui",
  "version": "1.0.0",
  "description": "UI redesign for the Figma to Code plugin, written in Tailwind",
  "main": "index.html",
  "scripts": {
    "source": "postcss tailwind.css -o styles.src.css",
    "min": "cross-env NODE_ENV=production postcss tailwind.css -o styles.min.css",
    "all": "npm run source && npm run min",
    "config": "tailwind-config-viewer -o"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "tailwind-config-viewer": "^1.5.0",
    "tailwind-scrollbar": "^1.2.2",
    "tailwindcss": "^2.0.2",
    "autoprefixer": "^10.1.0"
  },
  "dependencies": {
    "feather-icons": "^4.28.0",
    "highlight.js": "10.5.0"
  }
}
